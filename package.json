{
  "name": "homeblog-admin",
  "version": "1.0.0",
  "description": "homeblog-admin",
  "scripts": {
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "start": "node src/app/server/server.js",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register --require ./src/js/client/test/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node src/app/server/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miaomiao8890/homeblog-admin.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "webpack"
  ],
  "author": " sunchi",
  "license": "MIT",
  "main": "server.js",
